					<div class="form-group  usernameDiv">
						<div class="col-sm-10">
							<textarea type="education" class="form-control" name="education"
								id="lastName" placeholder="education"></textarea>
						</div>
					</div>

					<div class="form-group  usernameDiv">
						<div class="col-sm-10">
							<textarea type="skills" class="form-control" name="skills" id="skills"
								placeholder="skills"></textarea>
						</div>
					</div>

					<div class="form-group  usernameDiv">
						<div class="col-sm-10">
							<textarea type="experience" class="form-control" name="experience"
								id="experience" placeholder="experience"></textarea>
						</div>
					</div>

					<div class="form-group ">
						<div class="pull-left signIn">
							<button type="submit" class="btn btn-primary btn-sm btn">Update Profile</button>
						</div>
					</div>
				</form>
			</div>
		</div>
	</form>
